package project;

import processing.core.PApplet;

public class project extends PApplet {

    public static void main(String[] args) {
        PApplet.main("project.project");
    }

    public void settings() {
        size(400, 400);
    }

    public void setup() {

    }

    public void draw() {
        background(250);
        noStroke();
        drawPlayer();
    }

    public void drawPlayer() {
        fill(0, 230, 172);
        rectMode(CENTER);
        rect(30, 30, 30, 30);
    }
}
