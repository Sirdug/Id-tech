class Main {
    public static void main(String[] args)
    { 
      //Declare variables
      int strength;
      double health;
      String name;

      //assign varables
      strength = 4;
      health = 12.5;
      name = "The Ember Shadow";

      //print values
      System.out.println("Strength: " + strength);
      System.out.println("Health: " + health );
      System.out.println("Superhero Name: " + name);
    }
    
}
